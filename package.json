{
  "author": "Travis Illig",
  "dependencies": {
    "node-plantuml": "^0.9.0",
    "watch": "^1.0.2"
  },
  "description": "Example showing how to get PlantUML diagrams into an Azure DevOps wiki.",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": 1,
      "eqeqeq": 2,
      "no-console": 0,
      "no-debugger": 1,
      "no-extra-parens": 1,
      "no-extra-semi": 1,
      "no-irregular-whitespace": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "semi": 1,
      "semi-spacing": 1,
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false
        }
      ]
    }
  },
  "license": "MIT",
  "main": "index.js",
  "name": "plantuml-in-azdo-wiki",
  "repository": {
    "type": "git",
    "url": "https://github.com/tillig/plantuml-in-azdo-wiki.git"
  },
  "scripts": {
    "watch": "node index.js"
  },
  "version": "1.0.0"
}
